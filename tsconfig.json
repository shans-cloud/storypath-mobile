{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.jsx",
    "navigation/DrawerNavigator.jsx",
    "services/api.js",
    "components/Header.jsx",
    "app/App.jsx",
    "app/_layout.jsx",
    "app/(tabs)/About.jsx",
    "app/(tabs)/Home.jsx",
    "nativewind-env.d.ts"
  ]
}